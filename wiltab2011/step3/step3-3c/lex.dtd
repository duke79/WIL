<?xml version="1.0" encoding="UTF-8"?>

<!ELEMENT LEX (r)*>
<!ELEMENT r (rule,c,wil,mw)*>

<!ELEMENT rule (#PCDATA)>
<!ATTLIST rule conf (SURE|LIKELY|ROOT) #IMPLIED>
<!ATTLIST rule class (ERROR|VARIANT|GRAMMAR|ISSUE) #IMPLIED>
<!ATTLIST rule corrected (y) #IMPLIED>

<!ELEMENT c (#PCDATA)>
<!ATTLIST c auth (MA|SS|PS|Ss) #IMPLIED>
<!ATTLIST c cclass (E|G|V|N|R|Q|_|W|y) #IMPLIED>

<!ELEMENT wil (#PCDATA)>
<!ATTLIST wil err CDATA #IMPLIED>
<!ATTLIST wil corr CDATA #IMPLIED>

<!ELEMENT mw (#PCDATA)>

